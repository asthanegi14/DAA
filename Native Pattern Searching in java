public class NativePatternSearching {
    public static void main(String arg[])
    {
        String s = "AABAACAADAABAAABAA";
        String s2 = "AABA";

        int l1 = s.length();
        int l2 = s2.length();
        int i,j;
        for(i=0,j=l2-1;j<l1;)
        {
            if(s2.equals(s.substring(i,j+1)))
            {
                System.out.println("Index : "+i);
            }
            i++;j++;
        }
    }
}
